{
  "name": "ubair-website",
  "version": "1.0.0",
  "description": "Uintah Basin Air Quality Website",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js --ignore public/data/outlooks/outlooks_list.json --ignore public/api/static/",
    "test-api": "NODE_OPTIONS='-r dotenv/config' node scripts/test-api.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:snow": "NODE_OPTIONS='--experimental-vm-modules' jest snowDetection.test.js",
    "test:confidence": "NODE_OPTIONS='--experimental-vm-modules' jest confidenceThresholds.test.js"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "marked": "^14.1.3",
    "multer": "^2.0.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "plotly.js": "^2.35.3",
    "plotly.js-dist": "^2.35.2",
    "react-plotly.js": "^2.6.0",
    "recharts": "^2.14.1",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7"
  }
}
